<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kangkrkr.site.dao.mybatis.PersonDao">
	
	<select id="getPerson" resultType="com.kangkrkr.site.domain.Person">
		SELECT 
		       IDX 
		     , NAME 
		     , EMAIL 
		     , PASSWD
		  FROM PERSON 
		  ORDER BY IDX DESC
	</select>
   
 </mapper>